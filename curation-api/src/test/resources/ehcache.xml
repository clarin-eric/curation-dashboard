<!--
copied from ehcache/ehcache.xml to module test/resources
-->
<ehcache:config
        xmlns:ehcache="http://www.ehcache.org/v3"
        xmlns:jcache="http://www.ehcache.org/v3/jsr107">
    <ehcache:persistence directory="/tmp/ehcache"/>
    <ehcache:cache alias="ccrCache">
        <ehcache:key-type>java.lang.String</ehcache:key-type>
        <ehcache:value-type>java.io.Serializable</ehcache:value-type>
        <ehcache:expiry>
            <ehcache:none/>
        </ehcache:expiry>
        <ehcache:resources>
            <ehcache:disk persistent="true" unit="GB">1</ehcache:disk>
        </ehcache:resources>
    </ehcache:cache>
    <ehcache:cache alias="pphCache">
        <ehcache:key-type>java.lang.String</ehcache:key-type>
        <ehcache:value-type>java.io.Serializable</ehcache:value-type>
        <ehcache:expiry>
            <ehcache:none/>
        </ehcache:expiry>
        <ehcache:resources>
            <ehcache:disk persistent="true" unit="GB">1</ehcache:disk>
        </ehcache:resources>
    </ehcache:cache>
    <ehcache:cache alias="profileCache">
        <ehcache:key-type>java.lang.String</ehcache:key-type>
        <ehcache:value-type>java.io.Serializable</ehcache:value-type>
        <ehcache:expiry>
            <ehcache:none/>
        </ehcache:expiry>
        <ehcache:resources>
            <ehcache:disk persistent="true" unit="GB">1</ehcache:disk>
        </ehcache:resources>
    </ehcache:cache>
    <ehcache:cache alias="crCache">
        <ehcache:expiry>
            <ehcache:none/>
        </ehcache:expiry>
        <ehcache:heap unit="entries">500</ehcache:heap>
    </ehcache:cache>
    <ehcache:cache alias="profileReportCache">
        <ehcache:expiry>
            <ehcache:none/>
        </ehcache:expiry>
        <ehcache:heap unit="entries">500</ehcache:heap>
    </ehcache:cache>
    <ehcache:cache alias="facetMappingCache">
        <ehcache:expiry>
            <ehcache:none/>
        </ehcache:expiry>
        <ehcache:heap unit="entries">500</ehcache:heap>
    </ehcache:cache>
</ehcache:config>